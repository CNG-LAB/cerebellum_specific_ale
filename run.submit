# Environment
universe       = vanilla
getenv         = True
request_gpus   = 1
request_memory = 16GB

# Execution
initial_dir    = /data/project/cerebellum_ale
executable     = /bin/bash

# Job
arguments = $(initial_dir)/scripts/run.sh $(analysis) $(bd) $(subbd) $(n_subsamples)
log       = $(initial_dir)/logs/run_$(analysis)_$(bd)_$(subbd)_subsamples-$(n_subsamples).$(Cluster).$(Process).log
output    = $(initial_dir)/logs/run_$(analysis)_$(bd)_$(subbd)_subsamples-$(n_subsamples).$(Cluster).$(Process).out
error     = $(initial_dir)/logs/run_$(analysis)_$(bd)_$(subbd)_subsamples-$(n_subsamples).$(Cluster).$(Process).err
Queue